@model ProjectSetting

<div class="wrapper wrapper-content animated fadeInRight" ng-controller="ProjectSettingCtrl">
    <div class="row" ng-init="InitEdit()">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                @using (Html.BeginForm())
                {
                    <div class="ibox-title">
                        <h5>@Html.EditPageTitle("ProjectSetting")</h5>
                        <div class="ibox-tools">
                            @Html.SaveLinks()
                        </div>
                    </div>
                    <div class="ibox-content">
                        @Html.Partial("_ProjectSettingEdit", Model)
                    </div>
                }
            </div>
        </div>
    </div>
</div>

