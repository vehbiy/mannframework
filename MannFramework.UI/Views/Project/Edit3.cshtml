@model Project

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                @using (Html.BeginForm())
                {
                    <div class="ibox-title">
                        <h5>Edit Project</h5>
                        <div class="ibox-tools">
                            @Html.SaveLinks()
                        </div>
                    </div>
                    <div class="ibox-content">
                        @Html.Partial("_ProjectEdit", Model)
                    </div>
                }
            </div>
        </div>
    </div>
</div>
